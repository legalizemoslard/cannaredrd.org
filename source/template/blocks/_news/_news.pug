section#news.section.news

	.container
		.row.margin-bottom

			.col-12

				.heading.heading--primary.heading--center
					span.heading__pre-title Latest Articles 
					h2.heading__title.no-margin-bottom #[span CannaReD] #[span Blog]

		.row.offset-margin#newsContainer(style="margin-bottom: 30px;")

		.row.margin-bottom

			.col-12

				.heading.heading--primary.heading--center
					a.button.button--primary(href="https://medium.com/@cannared" target="_blank") Ver todos los Articulos

section#news.section.docs

	.container
		.row.margin-bottom

			.col-12

				.heading.heading--primary.heading--center
					span.heading__pre-title Latest Docs 
					h2.heading__title.no-margin-bottom #[span CannaReD] #[span Documentos]

		.row.offset-margin#newsContainer(style="margin-bottom: 30px;")

		.row.margin-bottom

			.col-md-6.col-lg-5.col-xl-4

				.blog-item.blog-item--style-1
